#version 410 core

// Input vertex data, different for all executions of this shader.
uniform float time;
out float someFloat;
layout(location = 0) in vec3 position;

void main(){
    gl_Position = vec4(position, 1.0) + vec4(sin(time) * 0.5, cos(time) * 0.5, 0.0, 0.0);
    someFloat = position.x + 0.5;
}